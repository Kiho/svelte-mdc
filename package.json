{
  "name": "svelte-mdc",
  "description": "Material Design components for Svelte",
  "version": "0.2.5",
  "author": "Kiho Chang",
  "license": "MIT",
  "module": "dist/svelte-mdc.mjs",
  "svelte": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiho/svelte-mdc.git"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "autolib": "rollup -c rollup.lib.config.js -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "release": "standard-version",
    "prepublishOnly": "run-p build"
  },
  "peerDependencies": {
    "svelte": ">= 3.5.0"
  },
  "dependencies": {
    "material-components-web": ">= 2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-plugin-svelte3": "^2.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.13.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "sirv-cli": "^0.4.4",
    "standard": "^10.0.3",
    "standard-version": "^8.0.1",
    "svelte": "^3.6.7"
  },
  "main": "dist/bundle.js",
  "files": [
    "dist",
    "src"
  ]
}
